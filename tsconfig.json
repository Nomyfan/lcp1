{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2020",
    "lib": ["esnext"],
    "noImplicitAny": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "src"
  },
  "include": ["src"]
}
